{
  "name": "reynard-algorithms",
  "version": "0.3.0",
  "description": "Algorithm implementations and data structures",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc -p tsconfig.json --emitDeclarationOnly --skipLibCheck",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly --skipLibCheck",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage --reporter=verbose",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint src examples --ext .ts,.tsx",
    "lint:fix": "eslint src examples --ext .ts,.tsx --fix",
    "format": "prettier --write src examples",
    "format:check": "prettier --check src examples",
    "examples": "tsx examples/basic-usage.ts",
    "examples:game": "tsx examples/game-engine-integration.ts",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "algorithms",
    "data-structures",
    "typescript"
  ],
  "author": "RuneClaw17",
  "license": "MIT",
  "homepage": "https://github.com/RuneClaw17/reynard-algorithms",
  "repository": {
    "type": "git",
    "url": "https://github.com/RuneClaw17/reynard-algorithms.git"
  },
  "bugs": {
    "url": "https://github.com/RuneClaw17/reynard-algorithms/issues"
  },
  "dependencies": {
    "solid-js": "1.9.9"
  },
  "devDependencies": {
    "@types/node": "24.7.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "happy-dom": "19.0.2",
    "reynard-testing": "^0.43.0",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-plugin-solid": "2.11.9",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "solid-js": "1.9.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
